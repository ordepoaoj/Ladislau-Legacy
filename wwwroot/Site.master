                        <li><a runat="server" href="~/Relatorio/endereco"><font color=#595959>Etiquetas</font></a></li>
                        <li><a runat="server" href="~/Preferencia"><font color=#595959>Preferencias</font></a></li>

                            </LoggedInTemplate>
